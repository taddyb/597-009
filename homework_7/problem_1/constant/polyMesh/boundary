5
(
    left
    {
        type            outletInlet;
        nFaces          10;
        startFace       180;
    }
    right
    {
        type            inletOutlet;
        nFaces          10;
        startFace       190;
    }
    up
    {
        type            slip;
        inGroups        List<word> 1(slip);
        nFaces          10;
        startFace       200;
    }
    down
    {
        type            slip;
        inGroups        List<word> 1(slip);
        nFaces          10;
        startFace       210;
    }
    frontAndBack
    {
        type            empty;
        inGroups        List<word> 1(empty);
        nFaces          200;
        startFace       220;
    }
)

