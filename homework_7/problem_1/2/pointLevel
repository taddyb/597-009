FoamFile
{
    version     2.0;
    format      ascii;
    class       pointScalarField;
    location    "2";
    object      pointLevel;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            calculated;
    }
    right
    {
        type            calculated;
    }
    up
    {
        type            calculated;
    }
    down
    {
        type            calculated;
    }
    frontAndBack
    {
        type            empty;
    }
}


