FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    left
    {
        type            outletInlet;
        outletValue     uniform (1 0 0);
    }

    right
    {
        type            inletOutlet;
        inletValue      uniform (1 0 0);
    }

    down
    {
        type            slip;
    }

    up
    {
        type           slip;
    }

    frontAndBack
    {
        type           empty;
    }
}
