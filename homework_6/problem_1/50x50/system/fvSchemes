FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         none;
    grad(T)         Gauss linear;
}

divSchemes
{
    default         none;
    div((rho*phi),T)      Gauss upwind;
}

laplacianSchemes
{
    default         none;
    laplacian(DT,T) Gauss linear corrected;
}

interpolationSchemes
{
    default         none;
    flux(U)         linear;
}

snGradSchemes
{
    default         none;
}

fluxRequired
{
    default         no;
    T               ;
}

